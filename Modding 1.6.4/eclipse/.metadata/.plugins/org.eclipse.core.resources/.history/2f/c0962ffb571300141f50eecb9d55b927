		world.setBlock(i + 5, j + 2, k + 5, Block.wood.blockID);
		world.setBlock(i + 5, j + 3, k + 5, Block.wood.blockID);
		world.setBlock(i + 5, j + 4, k + 3, Block.wood.blockID, 8,3);
